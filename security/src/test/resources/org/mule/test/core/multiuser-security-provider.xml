<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">

    <object name="securityProvider" class="org.mule.functional.security.TestMultiuserSecurityProvider"/>

    <security-manager>
        <custom-security-provider name="testSecurity" provider-ref="securityProvider"/>
        <password-encryption-strategy name="PBE" password="mule"/>
    </security-manager>

</mule>
