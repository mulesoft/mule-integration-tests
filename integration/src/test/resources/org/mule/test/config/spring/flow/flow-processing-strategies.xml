<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">


    <flow name="defaultFlow">
        <logger />
    </flow>

    <flow name="synchronousFlow" processingStrategy="synchronous">
        <logger />
    </flow>

    <flow name="customProcessingStrategyFlow" processingStrategy="customStrategy">
        <logger />
    </flow>
    
    <flow name="defaultAsync">
        <async>
            <logger />
        </async>
    </flow>
    
    <custom-processing-strategy name="customStrategy" class="org.mule.test.config.spring.flow.FlowProcessingStrategyConfigTestCase$CustomProcessingStrategyFactory">
        <property key="foo" value="bar" />
    </custom-processing-strategy>

</mule>
