<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd">

    <bean name="txFactory" class="org.mule.tck.testmodels.mule.TestTransactionFactory"/>

    <bean name="apple" class="org.mule.tck.testmodels.fruit.Apple"/>

    <bean name="orange" class="org.mule.tck.testmodels.fruit.Orange">
        <property name="brand" value="Juicy Baby!"/>
        <property name="segments" value="9"/>
        <property name="radius" value="4.21"/>
        <property name="mapProperties">
            <map>
                <entry key="prop1" value="prop1"/>
                <entry key="prop2" value="prop2"/>
            </map>
        </property>
        <property name="listProperties">
            <list>
                <value>prop1</value>
                <value>prop2</value>
                <value>prop3</value>
            </list>
        </property>
        <property name="arrayProperties">
            <list>
                <value>prop4</value>
                <value>prop5</value>
                <value>prop6</value>
            </list>
        </property>
    </bean>


    <bean name="anApple" class="org.mule.tck.testmodels.fruit.Apple"/>

    <bean name="prop-test-ref" class="org.mule.tck.testmodels.fruit.Apple">
        <property name="bitten" value="true"/>
        <property name="washed" value="true"/>
    </bean>

    <bean name="factory-test" class="org.mule.tck.testmodels.mule.TestDependentObject"/>

    <bean name="factoryObject" factory-bean="factory-test" factory-method="create">
        <constructor-arg>
            <map>
                <entry key="test1" value="test1"/>
                <entry key="test2" value="test2"/>
                <entry key="containerObject" value-ref="anApple"/>
                <entry key="osVersion" value="${os.version}"/>
                <!-- no defaults -->
                <entry key="propertiesMap">
                    <map>
                        <entry key="test1" value="test1"/>
                        <entry key="test2" value="test2"/>
                        <entry key="containerObject" value-ref="anApple"/>
                        <entry key="osVersion" value="${os.version}"/>
                        <!-- no nested factory (could repeat, but life is short...) -->
                    </map>
                </entry>
                <entry key="propertiesList">
                    <list>
                        <value>test1</value>
                        <value>test2</value>
                        <ref bean="anApple"/>
                        <value>${os.version}</value>
                    </list>
                </entry>
            </map>
        </constructor-arg>
    </bean>


</beans>
